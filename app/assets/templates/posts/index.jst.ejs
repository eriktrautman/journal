  <h1> This is the real Index Page </h1>
  <ul>
    <% posts.each(function(post){ %>
      <li>
        <ul>
          <li><%= post.get("title") %></li>
          <li><%= post.get("body") %></li>
          <li>
            <button class="delete" data-id="<%= post.get("id") %>">
              Delete
            </button>
          </li>
        </ul>
      </li>

    <% }) %>
  </ul>